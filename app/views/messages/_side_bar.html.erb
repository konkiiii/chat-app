<div class="header_side_bar">
  <div class="user_name">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class="create_chat">
    <%# <a href="#">チャットを作成する</a> %>
    <%= link_to "チャットを作成する", new_room_path %>
  </div>
</div>
<div class="center_side_bar">
  <% current_user.rooms.each do |room| %>
    <div class="chat_room">
      <a href="#"><%= room.name %></a>
    </div>
  <% end %>
</div>
